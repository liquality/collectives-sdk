{
  "compilerOptions": {
    "incremental": true,
    "declaration": true,
    "sourceMap": false,
    "composite": true,
    "types": ["node", "jest"],
    "typeRoots": ["./node_modules/@types"],
    "esModuleInterop": true,
    "noImplicitAny": false,
    "strict": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true
  }
}
